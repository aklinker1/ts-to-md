{
  "compilerOptions": {
    "target": "ESNext",
    "module": "esnext",
    "lib": ["dom", "ESNext"],
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "types": ["node"]
  },
  "exclude": ["lib"]
}
