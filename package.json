{
  "name": "ts-to-md",
  "description": "Generate markdown docs from a typescript file",
  "version": "0.0.1",
  "license": "MIT",
  "author": "Aaron Klinker",
  "repository": "https://github.com/aklinker1/ts-to-md",
  "keywords": [
    "typescript",
    "documentation",
    "generator",
    "markdown"
  ],
  "type": "module",
  "main": "lib/index.cjs",
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "bin": {
    "ts-to-md": "lib/cli.js"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "test": "vitest --outputDiffLines 1000",
    "build": "tsup src/cli.ts src/index.ts --outDir lib --clean --format cjs,esm --dts",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "husky install",
    "prepublish": "pnpm build",
    "gen:docs": "pnpm build && node lib/cli.js src/index.ts API.md"
  },
  "devDependencies": {
    "@types/prettier": "^2.7.2",
    "husky": "^8.0.3",
    "pretty-quick": "^3.1.3",
    "tsup": "^6.5.0",
    "typescript": "^4.9.5",
    "vitest": "^0.28.4"
  },
  "dependencies": {
    "@types/node": "18",
    "meow": "^11.0.0",
    "prettier": "^2.8.3",
    "ts-morph": "^17.0.1"
  }
}
